[package]
name = "carnix"
version = "0.6.8"
authors = ["pe@pijul.org <pe@pijul.org>"]
include = ["Cargo.toml", "src/main.rs", "src/cache.rs", "src/cfg.rs", "src/krate/mod.rs", "src/krate/prefetch.rs", "src/preamble.nix" ]
description = "Generate Nix expressions from Cargo.lock files (in order to use Nix as a build system for crates)."
homepage = "https://nest.pijul.com/pmeunier/nix-rust"
license = "MIT/Apache-2.0"
repository = "https://nest.pijul.com/pmeunier/nix-rust"
[dependencies.clap]
version = "2.27"

[dependencies.serde]
version = "1.0"

[dependencies.toml]
version = "0.4"

[dependencies.serde_json]
version = "1.0"

[dependencies.env_logger]
version = "0.5"

[dependencies.error-chain]
version = "0.11"

[dependencies.nom]
version = "3.2"

[dependencies.log]
version = "0.4"

[dependencies.regex]
version = "0.2"

[dependencies.serde_derive]
version = "1.0"

[dependencies.rusqlite]
version = "0.13"

[dependencies.tempdir]
version = "0.3"

[dependencies.itertools]
version = "0.7.2"

